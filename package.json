    "swiper": "^11.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.0.2",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
